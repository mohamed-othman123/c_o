{
  "name": "@corp-web-monorepo/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "is-ci || husky",
    "*** Dev *****": "******* Developer Friendly scripts *****",
    "reset": "pnpm exec nx reset",
    "start": "pnpm exec nx run-many --target=serve --projects=ebanking-portal,mock-api",
    "start:dev": "pnpm exec nx run ebanking-portal:serve:dev",
    "start:sit": "pnpm exec nx run ebanking-portal:serve:sit",
    "format:all": "prettier --write \"**/*.{ts,js,scss,css,html,md,json,yml}\"",
    "format:check": "prettier --check \"**/*.{ts,js,scss,css,html,md,json,yml}\"",
    "lint": "nx run ebanking-portal:lint",
    "cm": "git cz",
    "build": "nx run ebanking-portal:build",
    "test": "nx run ebanking-portal:test",
    "cov": "pnpm exec nx run ebanking-portal:test --watchAll=true --codeCoverage=true",
    "setup:config": "node config-setup.js",
    "release": "pnpm exec nx run ebanking-portal:release",
    "release:sit": "pnpm exec nx run ebanking-portal:release:sit",
    "**** Test ****": "******* unit test and coverage reports scripts *****",
    "test:coverage": "pnpm run test --coverage --passWithNoTests",
    "**** Build ****": "******* Build and Deploy scripts *****",
    "build:ci": "nx run ebanking-portal:build",
    "test:ci": "pnpm run test --coverage --passWithNoTests --coverageReporters=cobertura",
    "**** UTIL ****": "******* UTIL *****",
    "build:analyze": "nx build ebanking-portal --stats-json",
    "clean:repo": "git clean -fd",
    "clean:repo:remote": "git remote prune origin",
    "clean:repo:old:branches": "git fetch -p && git branch -vv | awk '/: gone]/{print $1}' | xargs git branch -D"
  },
  "private": true,
